{
    "$schema": "vscode://schemas/color-theme",
    "name": "Plain Light Theme",
    "colors": {
        "editor.background": "#fbf8f1",
        "editor.foreground": "#75756e",
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": { "foreground": "#bf6e3e" }
        },
        {
            "scope": "string",
            "settings": { "foreground": "#2a9667" }
        },
        {
            "scope": "constant",
            "settings": { "foreground": "#8678c9" }
        },
        {
            "scope": "keyword",
            "settings": { "foreground": "#488acb" }
        },
        {
            "scope": "storage",
            "settings": { "foreground": "#488acb" }
        },
        {
            "scope": "entity.name.type",
            "settings": { "foreground": "#2a9cae" }
        },
        {
            "scope": "support.type",
            "settings": { "foreground": "#2a9cae" }
        }
    ]
}