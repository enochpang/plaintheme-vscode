{
    "$schema": "vscode://schemas/color-theme",
    "name": "Plain Light Theme",
    "colors": {
        "editor.background": "#fbf8f1",
        "editor.foreground": "#716e68",
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": { "foreground": "#bf6e3e" }
        },
        {
            "scope": "string",
            "settings": { "foreground": "#339c6d" }
        },
        {
            "scope": "constant",
            "settings": { "foreground": "#9274c3" }
        },
        {
            "scope": "keyword",
            "settings": { "foreground": "#338fc7" }
        },
        {
            "scope": "storage",
            "settings": { "foreground": "#338fc7" }
        },
        {
            "scope": "entity.name.type",
            "settings": { "foreground": "#04a3aa" }
        },
        {
            "scope": "support.type",
            "settings": { "foreground": "#04a3aa" }
        }
    ]
}